{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "plugin:@wordpress/eslint-plugin/recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "@wordpress/eslint-plugin"
    ],
    "rules": {
        "prettier/prettier": ["off"],
        "no-duplicate-imports": "off",
        "object-shorthand": "warn",
        "no-nested-ternary": "off",
        "no-shadow": "warn",
        "no-console": "warn"
    }
}